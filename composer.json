{
    "name": "abdallahmohammed/laravel-eloquent-filter",
    "description": "A simple laravel package to filter models and relations",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "search",
        "query",
        "filter"
    ],
    "license": "MIT",
    "authors": [{
        "name": "AbdallahMohammed",
        "email": "abdallah.r660@gmail.com"
    }],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.3.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelEloquentFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEloquentFilter\\Providers\\LaravelServiceProvider"
            ]
        }
    }
}